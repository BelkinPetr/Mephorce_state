<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="HibernateEntities.InvitationEntity" table="invitations" schema="anketa">
        <id name="id">
            <column name="match_id" sql-type="varchar(45)"/>
        </id>
        <property name="stId">
            <column name="stud_id" sql-type="int(11)"/>
        </property>
        <property name="prId">
            <column name="proj_id" sql-type="int(11)"/>
        </property>
        <property name="result">
            <column name="result" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>