<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="HibernateEntities.ProjectsEntity" table="projects" schema="anketa">
        <id name="prId">
            <column name="pr_id" sql-type="int(11)"/>
        </id>
        <property name="title">
            <column name="title"/>
        </property>
        <property name="description">
            <column name="description"/>
        </property>
        <property name="dateOfCreation">
            <column name="date_of_creation" sql-type="timestamp"/>
        </property>
        <property name="status">
            <column name="status"/>
        </property>
        <property name="dateOfReady">
            <column name="date_of_ready" sql-type="date"/>
        </property>
        <property name="sum">
            <column name="sum" sql-type="int(11)"/>
        </property>
        <property name="jsondata">
            <column name="jsondata"/>
        </property>
        <property name="clId">
            <column name="client_id"/>
        </property>
        <property name="mdId">
            <column name="mod_id"/>
        </property>
        <property name="moneyDivision">
            <column name="moneyDiv"/>
        </property>
        <property name="personNumber">
            <column name="personNumber"/>
        </property>
    </class>
</hibernate-mapping>